{
  "name": "markdown-it-checkbox",
  "version": "1.1.1",
  "description": "Plugin to create checkboxes for markdown-it markdown parser",
  "repository": "mcecot/markdown-it-checkbox",
  "license": "MIT",
  "scripts": {
    "prepublish": "gulp coffee --require coffee-script/register",
    "test": "coffeelint gulpfile.coffee index.coffee test -f ./coffeelint.json",
    "coverage": "istanbul test node_modules/mocha/bin/_mocha -- ./test/*.coffee --require coffee-script/register --reporter spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown"
  ],
  "devDependencies": {
    "browserify": "14.3.0",
    "coffee-script": "1.12.5",
    "coffeelint": "1.16.0",
    "coveralls": "2.13.0",
    "del": "2.2.2",
    "eslint": "3.19.0",
    "gulp": "3.9.1",
    "gulp-bump": "2.7.0",
    "gulp-coffee": "2.3.4",
    "gulp-filter": "5.0.0",
    "gulp-git": "2.2.0",
    "gulp-tag-version": "1.3.0",
    "gulp-util": "3.0.8",
    "istanbul": "0.4.5",
    "markdown-it": "8.3.1",
    "markdown-it-testgen": "0.1.4",
    "mocha": "3.3.0",
    "should": "11.2.1",
    "uglify-js": "2.8.22"
  },
  "dependencies": {
    "underscore": "1.8.3"
  }
}
